#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/abc/Desktop/front/node_modules/.store/@electron+fuses@1.8.0/node_modules/@electron"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/abc/Desktop/front/node_modules/.store/@electron+fuses@1.8.0/node_modules/@electron"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@electron+fuses@1.8.0/node_modules/@electron/fuses/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@electron+fuses@1.8.0/node_modules/@electron/fuses/dist/bin.js" "$@"
fi
